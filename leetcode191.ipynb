   "source": [
    "class Solution:\n",
    "    def hammingWeight(self, n: int) -> int:\n",
    "        count = 0\n",
    "        while n != 0:\n",
    "            count += 1\n",
    "            n &= n-1\n",
    "            \n",
    "        return count"
   ]
