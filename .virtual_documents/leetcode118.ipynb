


# T:O(n^2), S:O(n^2)
from typing import List
class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
        ans = list()
        for i in range(numRows):
            curRow = list()
            for j in range(i+1):
                if j in (0,i):
                    curRow.append(1)
                else:
                    curRow.append(ans[-1][j-1]+ans[-1][j])
            ans.append(curRow)
                    
        return ans


