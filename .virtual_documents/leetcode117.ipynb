import collections

class Solution:
    def connect(self, root: 'Node') -> 'Node':
        if not root:
            return root 
        Q = collections.deque([root])
        
        while Q:
            size = len(Q)
            
            for i in range(size):
                node = Q.popleft()
                if i < size-1:
                    node.next = Q[0]
                    
                if node.left:
                    Q.append(node.left)
                if node.right:
                    Q.append(node.right)
                
        return root


