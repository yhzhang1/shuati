   "source": [
    "class Solution:\n",
    "    def convertToTitle(self, n: int) -> str:\n",
    "        ans = ''\n",
    "        while n:\n",
    "            ans = chr(ord('A') + (n - 1) % 26) + ans\n",
    "            n = (n - 1) // 26\n",
    "            \n",
    "        return ans\n",
    "        "
   ]
