   "source": [
    "class Solution:\n",
    "    def rangeBitwiseAnd(self, m: int, n: int) -> int:\n",
    "        shift = 0\n",
    "        while m < n:\n",
    "            m >>= 1\n",
    "            n >>= 1\n",
    "            shift += 1\n",
    "        return m << shift"
   ]
  }
