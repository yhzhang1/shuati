   "source": [
    "class Solution:\n",
    "    def computeArea(self, A: int, B: int, C: int, D: int, E: int, F: int, G: int, H: int) -> int:\n",
    "        overlap = max(min(C,G) - max(A,E), 0) * max(min(D,H) - max(B,F), 0)\n",
    "        return (A-C)*(B-D) + (E-G)*(F-H) - overlap"
   ]
  }
